{
  "name": "localkit",
  "version": "1.4.7",
  "description": "The localkit package is an open-source command-line interface (CLI) designed to streamline your web development workflow. It provides a suite of tools for common tasks, including secure password generation, Base64URL encoding/decoding, UUID creation, OTP management, encryption, hashing, favicon creation, PWA asset generation, and Material Icon installation.",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "start": "node src/index.js",
    "test": "echo \"Error: tests are executed with npm run test:unit | npm run test:integration\" && exit 1",
    "test:unit": "jest --config=jest.test-unit.config.json",
    "test:integration": "jest --config=jest.test-integration.config.json"
  },
  "bin": {
    "localkit": "src/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jesusgraterol/localkit.git"
  },
  "author": "Jesus Graterol",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jesusgraterol/localkit/issues"
  },
  "homepage": "https://github.com/jesusgraterol/localkit#readme",
  "devDependencies": {
    "@babel/core": "7.26.0",
    "@babel/preset-env": "7.26.0",
    "@jest/globals": "29.7.0",
    "babel-jest": "29.7.0",
    "eslint": "8.57.1",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-prettier": "10.1.5",
    "eslint-plugin-import": "2.31.0",
    "jest": "29.7.0",
    "prettier": "3.6.2"
  },
  "dependencies": {
