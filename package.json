    "check-password-strength": "2.0.10",
    "date-fns": "2.30.0",
    "generate-password": "1.7.1",
    "md5": "2.3.0",
    "otplib": "12.0.1",
    "png-to-ico": "2.1.8",
    "sharp": "0.33.5",
